{
  "name": "@tbsp/web",
  "version": "0.0.0",
  "description": "A web server and client built with sockets which supplies the framework for the TBSP HTTP server and WS connections.",
  "type": "module",
  "exports": {
    ".": [
      "./dist/index.js",
      "./src/index.ts"
    ],
    "./middleware": [
      "./dist/middleware/index.js",
      "./src/middleware/index.ts"
    ],
    "./tbsp": [
      "./dist/tbsp.js",
      "./src/tbsp.ts"
    ],
    "./*.ts": "./src/*.ts",
    "./ws/*.ts": "./src/ws/*.ts"
  },
  "files": [
    "src/**/*"
  ],
  "license": "ISC",
  "scripts": {
    "test": "bun test --config=../../bunfig.toml",
    "qtest": "bun test --config=../../bunfig.toml 2>&1 | tail -n 5",
    "lint": "npx tsc --noEmit",
    "build": "npx tsc",
    "prepare": "npm run build"
  },
  "dependencies": {
    "bun": "^1.3.0",
    "urlpattern-polyfill": "^10.1.0"
  },
  "devDependencies": {
    "@types/bun": "^1.2.22"
  }
}
